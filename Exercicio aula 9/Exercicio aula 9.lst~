                        63     ;Função que converte o número no registrador R0 em BCD
                        64     ;Já aproveita e envia o valor para a memória externa
0041 E8                 65     CONV2BCD_R0:    MOV     A, R0
0042 75F00A             66                     MOV     B, #0AH
0045 84                 67                     DIV     AB
0046 F531               68                     MOV     31H, A  ; 31H = ISB1
0048 85F030             69                     MOV     30H, B  ; 30H = LSB
                        70
                        71                     ;Converte o valor de BCD para D7S
004B 9000B5             72                     MOV     DPTR, #D7SVALORES
004E 93                 73                     MOVC    A, @A+DPTR      ;O valor D7S está carregado no acumulador
                        74
                        75                     ;Move o valor correto para a posição de memoria respectiva
004F 908000             76                     MOV     DPTR, #8000H
0052 F0                 77                     MOVX    @DPTR, A
                        78
