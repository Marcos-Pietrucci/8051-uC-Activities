0055 9000B5             81                     MOV     DPTR, #D7SVALORES
0058 93                 82                     MOVC    A, @A+DPTR      ;O valor D7S está carregado no acumulador
                        83
                        84                     ;Move o valor correto para a posição de memoria respectiva
