                         8     ; Para armazenar o valor dos displays, usarei os registradores
                         9     ;       1000H - R0
                        10     ;       2000H - R1
                        11     ;       3000H - R2
                        12     ;       4000H - R3
                        13
                        14     ; R1 para os 2 mais significativos, R2 para os 2 leds menos significativos
                        15     ; Buffer: XDATA              INTERNO
                        16     ;         8000   D7S-0         30H
                        17     ;         8100   D7S-1         31H
                        18     ;         8200   D7S-2         32H
                        19     ;         8300   D7S-3         33H
                        20     ;         8400   D7S-4         34H
                        21     ;         8500   D7S-5         35H
                        22     ;         8600   D7S-6         36H
                        23     ;         8700   D7S-7         37H
                        24
                        25                     ORG     0H
0000 8000               26                     SJMP    SETUP
                        27
0002 D2AF               28     SETUP:          SETB    EA              ;Configura interrupções
0004 D2AA               29                     SETB    EX1
0006 D28A               30                     SETB    IT1
                        31
0008 7800               32                     MOV     R0, #0H         ;Contador menos significativo
000A 7900               33                     MOV     R1, #0H         ;Contador mais significativo
000C 7A00               34                     MOV     R2, #0H         ;Armazena qual o bit ativo
000E 753000             35                     MOV     30H, #0H        ;Limpa o buffer
0011 753100             36                     MOV     31H, #0H
0014 753200             37                     MOV     32H, #0H
0017 753300             38                     MOV     33H, #0H
001A 75A0FF             39                     MOV     P2, #0FFH       ;Limpa os ativadores
001D 7590FF             40                     MOV     P1, #0FFH
                        41
0020 112C               42     LOOP:           ACALL   LE_MEMORIA
0022 1141               43                     ACALL   CONV2BCD_R0
0024 115E               44                     ACALL   CONV2BCD_R1
0026 117B               45                     ACALL   CONV2BCD_R2
0028 1198               46                     ACALL   CONV2BCD_R3
002A 80F4               47                     SJMP    LOOP
                        48
002C 901000             49     LE_MEMORIA:     MOV     DPTR, #1000H
002F E0                 50                     MOVX    A, @DPTR
0030 F8                 51                     MOV     R0, A
0031 902000             52                     MOV     DPTR, #2000H
0034 E0                 53                     MOVX    A, @DPTR
0035 F9                 54                     MOV     R1, A
0036 903000             55                     MOV     DPTR, #3000H
