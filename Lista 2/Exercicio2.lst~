                       115     ;Subrotina que liga e configura o LCD
00A6 C2B4              116     INIT_LCD:       CLR     RW
00A8 C2B4              117                     CLR     RS
00AA D2B2              118                     SETB    ENAB
00AC 75A038            119                     MOV     DAT, #38H       ;2x16 e matriz 5x7
00AF C2B2              120                     CLR     ENAB
00B1 D2B2              121                     SETB    ENAB
00B3 75A00E            122                     MOV     DAT, #0EH       ;Display on/off
00B6 C2B2              123                     CLR     ENAB
00B8 D2B2              124                     SETB    ENAB
00BA 75A006            125                     MOV     DAT, #06H       ;Modo de entrada
00BD C2B2              126                     CLR     ENAB
00BF 22                127                     RET
                       128
                       129     ;**************************************************
00C0 C2B4              130     CLR_LCD:        CLR     RW
00C2 C2B4              131                     CLR     RS
00C4 D2B2              132                     SETB    ENAB
00C6 75A001            133                     MOV     DAT, #01H
00C9 C2B2              134                     CLR     ENAB
00CB 22                135                     RET
                       136     ;**************************************************
00CC C2B4              137     POS_LCD:        CLR     RW
00CE C2B4              138                     CLR     RS
00D0 D2B2              139                     SETB    ENAB
00D2 2480              140                     ADD     A, #80H
00D4 F5A0              141                     MOV     DAT, A
00D6 C2B2              142                     CLR     ENAB
00D8 22                143                     RET
