                        76     ;Função que converte o número no registrador R0 em BCD
0051 E8                 77     Conv2Digital:   MOV     A, R0
0052 B40005             78                     CJNE    A, #0H,Test1
0055 753000             79                     MOV     30H, #0H                        ;Tensão é 0
0058 804B               80                     SJMP    RETURN
005A B41705             81     Test1:          CJNE    A, #17H,Test2
005D 753001             82                     MOV     30H, #1H                        ;Tensão é 1v
0060 8043               83                     SJMP    RETURN
0062 B42E05             84     Test2:          CJNE    A, #2EH,Test3
0065 753002             85                     MOV     30H, #2H                        ;Tensão é 2v
0068 803B               86                     SJMP    RETURN
006A B44505             87     Test3:          CJNE    A, #45H,Test4
006D 753003             88                     MOV     30H, #3H                        ;Tensão é 3V
0070 8033               89                     SJMP    RETURN
0072 B45C05             90     Test4:          CJNE    A, #5CH,Test5
0075 753004             91                     MOV     30H, #4H                        ;Tensão é 4V
0078 802B               92                     SJMP    RETURN
007A B47305             93     Test5:          CJNE    A, #73H,Test6
007D 753005             94                     MOV     30H, #5H                        ;Tensão é 5V
0080 8023               95                     SJMP    RETURN
0082 B48A05             96     Test6:          CJNE    A, #8AH,Test7
0085 753006             97                     MOV     30H, #6H                        ;Tensão é 6V
0088 801B               98                     SJMP    RETURN
008A B4A105             99     Test7:          CJNE    A, #0A1H,Test8
008D 753007            100                     MOV     30H, #7H                        ;Tensão é 7V
0090 8013              101                     SJMP    RETURN
